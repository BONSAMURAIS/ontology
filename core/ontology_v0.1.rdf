<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:schema="http://schema.org/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:dc="http://purl.org/dc/terms/"
    xmlns:time="http://www.w3.org/2006/time#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:bont="http://ontology.bonsai.uno/core#"
    xmlns:cc="http://creativecommons.org/ns#"
    xmlns:ns0="http://purl.org/vocab/vann/"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:om2="http://www.ontology-of-units-of-measure.org/resource/om-2/" > 
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#referenceFlowObject">
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#FlowObject"/>
    <rdfs:label xml:lang="en">references flow object</rdfs:label>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#ReferenceUnit"/>
    <rdfs:comment xml:lang="en">The flow object referenced by the reference unit.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#proportionalTo">
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#ReferenceUnit"/>
    <rdfs:label xml:lang="en">proportional to</rdfs:label>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Flow"/>
    <rdfs:comment xml:lang="en">The reference unit to which the quantity of the Flow is proportional.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#Flow">
    <rdfs:subClassOf rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Measure"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:label xml:lang="en">Flow</rdfs:label>
    <rdfs:comment xml:lang="en">An observation representing a quantity of Flow Object that is consumed as input from or produced as output of an Activity.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#Activty">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:label xml:lang="en">Activity</rdfs:label>
    <rdfs:comment xml:lang="en">Activity is the act of doing something which is defined both spatially and temporally. Includes both human activities and environmental mechanisms.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A0">
    <rdf:rest rdf:nodeID="A1"/>
    <rdf:first rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#FlowObject">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:label xml:lang="en">Flow Object</rdfs:label>
    <rdfs:comment xml:lang="en">An entity that is produced or consumed by an activity.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#float">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A2">
    <owl:unionOf rdf:nodeID="A0"/>
    <rdfs:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A1">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="http://ontology.bonsai.uno/core#Agent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#inputOf">
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
    <rdfs:label xml:lang="en">input of</rdfs:label>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Flow"/>
    <rdfs:comment xml:lang="en">A flow that is input of an activity.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#objectType">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/type"/>
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#FlowObject"/>
    <rdfs:label xml:lang="en">object type</rdfs:label>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Flow"/>
    <rdfs:comment xml:lang="en">Type of the Flow Object that is consumed or produced.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#Agent">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:label xml:lang="en">Agent</rdfs:label>
    <rdfs:comment xml:lang="en">An entity that performs an activity, usually it has a specific location.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#BalanceableProperty">
    <rdfs:subClassOf rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Quantity"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:label xml:lang="en">Balanceable Property</rdfs:label>
    <rdfs:comment xml:lang="en">A balanceable property defines a type of quantity that should be balanced when input or output of an activity.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#location">
    <rdfs:range rdf:resource="http://schema.org/location"/>
    <rdfs:label xml:lang="en">located at</rdfs:label>
    <rdfs:domain rdf:nodeID="A2"/>
    <rdfs:comment xml:lang="en">The location in which an activity took place or agent is located.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#ReferenceUnit">
    <rdfs:subClassOf rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Measure"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:label xml:lang="en">Reference Unit</rdfs:label>
    <rdfs:comment xml:lang="en">The quantity to which the quantity of a Flow should be seen proportional to. This could be, for instance, 1 Year, or per tonne of steel.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#determiningFlow">
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#Flow"/>
    <rdfs:label xml:lang="en">determining flow</rdfs:label>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
    <rdfs:comment xml:lang="en">Specific flow object produced or consumed by an activity to deliver its primary function.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#ActivtyType">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:label xml:lang="en">Activity Type</rdfs:label>
    <rdfs:comment xml:lang="en">The type of an activity, e.g, stock accumulation, steel production, etc.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#temporalExtent">
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#ProperInterval"/>
    <rdfs:label xml:lang="en">temporal extent</rdfs:label>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
    <rdfs:comment xml:lang="en">Temporal extent for an activity.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#outputOf">
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
    <rdfs:label xml:lang="en">output of</rdfs:label>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Flow"/>
    <rdfs:comment xml:lang="en">A flow that is output of an activity.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core#performs">
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
    <rdfs:label xml:lang="en">performs</rdfs:label>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Agent"/>
    <rdfs:comment xml:lang="en">The activity performed by the agent.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.bonsai.uno/core">
    <rdfs:comment xml:lang="en">First ever version 0.1 :
                  Will change!
               </rdfs:comment>
    <owl:versionInfo xml:lang="en">Version 0.1 - 2019-03-25</owl:versionInfo>
