<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:foaf="http://xmlns.com/foaf/0.1/"
         xmlns:ns0="http://purl.org/vocab/vann/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:cc="http://creativecommons.org/ns#">

  <owl:Ontology rdf:about="http://ontology.bonsai.uno/core">
    <dc:title xml:lang="en">The BONSAI ontology</dc:title>
    <dc:description xml:lang="en">The BONSAI ontologies provides elements of description for product footprint concepts defined in the bonsai.uno data base</dc:description>
    <foaf:homepage rdf:resource="http://ontology.bonsai.uno/core/documentation.html"/>
    <ns0:preferredNamespacePrefix>bont</ns0:preferredNamespacePrefix>
    <ns0:preferredNamespaceUri>http://ontology.bonsai.uno/core#</ns0:preferredNamespaceUri>
    <rdfs:isDefinedBy rdf:resource="http://ontology.bonsai.uno/core/ontology_v0.1.rdf"/>
    <owl:versionInfo xml:lang="en">Version 0.1 - 2019-03-25</owl:versionInfo>
    <dc:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-03-25</dc:modified>
    <dc:publisher>bonsai.uno</dc:publisher>
    <dc:creator rdf:resource="http://bonsai.uno/foaf/bonsai.rdf#bonsai"/>
    <dc:contributor>Matteo Lissandrini</dc:contributor>
    <dc:contributor>Massimo Pizzol</dc:contributor>
    <dc:contributor>Agneta Ghose</dc:contributor>
    <cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <rdfs:comment xml:lang="en">First ever version 0.1 :
                  Will channge!
               </rdfs:comment>
  </owl:Ontology>

  <owl:Class rdf:about="http://ontology.bonsai.uno/core#Activty">
    <rdfs:comment xml:lang="en">Activity is the act of doing something which is defined both spatially and temporally. Includes both human activities and environmental mechanisms.</rdfs:comment>
    <rdfs:label xml:lang="en">Activity</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
  </owl:Class>

  <owl:Class rdf:about="http://ontology.bonsai.uno/core#ActivtyType">
    <rdfs:comment xml:lang="en">The type of an activity, e.g, stock accumulation, steel production, etc.</rdfs:comment>
    <rdfs:label xml:lang="en">Activity Type</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
  </owl:Class>

  <owl:Class rdf:about="http://ontology.bonsai.uno/core#FlowObject">
    <rdfs:comment xml:lang="en">An entity that is produced or consumed by an activity.</rdfs:comment>
    <rdfs:label xml:lang="en">Flow Object</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
  </owl:Class>

  <owl:Class rdf:about="http://ontology.bonsai.uno/core#Flow">
    <rdfs:comment xml:lang="en">An observation representing a quantity of Flow Object that is consumed as input from or produced as output of an Activity.</rdfs:comment>
    <rdfs:label xml:lang="en">Flow</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:subClassOf rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/Quantity"/>
  </owl:Class>

  <owl:Class rdf:about="http://ontology.bonsai.uno/core#Agent">
    <rdfs:comment xml:lang="en">An entity that performs an acitivity, usually it has a specific location.</rdfs:comment>
    <rdfs:label xml:lang="en">Agent</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
  </owl:Class>

  <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#integer">
  </rdfs:Datatype>

  <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#float">
  </rdfs:Datatype>

  <owl:FunctionalProperty rdf:about="http://ontology.bonsai.uno/core#inputOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">A flow that is input of an activity.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Flow"/>
    <rdfs:label xml:lang="en">input of</rdfs:label>
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
  </owl:FunctionalProperty>

  <owl:FunctionalProperty rdf:about="http://ontology.bonsai.uno/core#outputOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">A flow that is output of an activity.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Flow"/>
    <rdfs:label xml:lang="en">output of</rdfs:label>
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
  </owl:FunctionalProperty>

  <owl:FunctionalProperty rdf:about="http://ontology.bonsai.uno/core#objectType">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Type of the Flow Object that is consumed or produced.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Flow"/>
    <rdfs:label xml:lang="en">object type</rdfs:label>
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#FlowObject"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/type"/>
  </owl:FunctionalProperty>

  <owl:ObjectProperty rdf:about="http://ontology.bonsai.uno/core#activityType">
    <rdfs:comment xml:lang="en">Type of the Activity that is peformed.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
    <rdfs:label xml:lang="en">object type</rdfs:label>
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#ActivityType"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/type"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://ontology.bonsai.uno/core#performs">
    <rdfs:comment xml:lang="en">The activity performed by the agent.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Agent"/>
    <rdfs:label xml:lang="en">performs</rdfs:label>
    <rdfs:range rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
  </owl:ObjectProperty>

  <owl:FunctionalProperty rdf:about="http://ontology.bonsai.uno/core#temporalExtent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Temporal extent for an activity.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
    <rdfs:label xml:lang="en">temporal extent</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#ProperInterval"/>
  </owl:FunctionalProperty>

  <owl:FunctionalProperty rdf:about="http://ontology.bonsai.uno/core#location">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">The location in which an activity took place.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontology.bonsai.uno/core#Activity"/>
    <rdfs:label xml:lang="en">location</rdfs:label>
    <rdfs:range rdf:resource="http://schema.org/location"/>
  </owl:FunctionalProperty>

</rdf:RDF>
